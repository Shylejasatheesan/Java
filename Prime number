// Prime number
import java.util.Scanner; 
public class primeNumber 
{ 
public static void main(String args[]) 
{ 
Scanner s=new Scanner(System.in); 
System.out.println("Enter a no. :");
int n=s.nextInt(); if(n==0||n==1) 
System.out.println(n+" is not a prime no."); else 
{ int i=2,flag=0; for(;i<=n/2;i++) 
{ if(n%i==0) 
{ flag=1; break; 
} 
} if(flag==0) 
System.out.println(n+" is a prime no."); else  
 	System.out.println(n+" is not a prime no."); 
} 
s.close(); 
} 
} 
